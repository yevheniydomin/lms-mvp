{
  "name": "lms-mvp",
  "version": "0.1.0",
  "workspaces": [
    "app/"
  ],
  "bin": {
    "lms-mvp": "bin/stack.ts"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "pretty": "npx prettier --write \"./**/*.{js,jsx,json,ts}\"",
    "lint": "npx eslint -c .eslintrc.js --ext .ts .",
